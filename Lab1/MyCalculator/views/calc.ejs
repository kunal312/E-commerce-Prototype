<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>My Calculator</title>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular.min.js"></script>
<link rel="stylesheet" href="stylesheets/style.css">
<script src="calculation/calci.js"></script>

</head>
<div class="text-center">
<h1>Calculator</h1>
</div>
<div ng-app=myApp ng-controller="Calculator">
<div id="calcOutput" >
  <p class="output" name="output">{{output}}
  <br>
  {{operationToken}}
 <br>
 {{result}}
   
                <!-- <span id="steps">0</span> -->

<hr/>
</div>
<div class="text-center" id="calculator">
<a class="btn btn-danger bigButton" id="deleteALL" ng-click='deleteALL()' >AC</a>
<a class="btn btn-danger" id="backOne" ng-click='backone()'>CE</a>
<a class="btn btn btn btn-warning" id="/" ng-click='division()'>/</a>
<br>
<a class="btn btn-primary" id="7" ng-click='updateOutput(7)'>7</a>
<a class="btn btn-primary" id="8" ng-click='updateOutput(8)'>8</a>
<a class="btn btn-primary" id="9" ng-click='updateOutput(9)'>9</a>
<a class="btn btn btn-warning" id="*" ng-click='multiply()'>*</a>
<br>
<a class="btn btn-primary" id="4" ng-click='updateOutput(4)'>4</a>
<a class="btn btn-primary" id="5" ng-click='updateOutput(5)'>5</a>
<a class="btn btn-primary" id="6" ng-click='updateOutput(6)' >6</a>
<a class="btn btn btn btn-warning" id="plus" ng-click='add()'>+</a>
<br>
<a class="btn btn-primary" id="1" ng-click='updateOutput(1)'>1</a>
<a class="btn btn-primary" id="2" ng-click='updateOutput(2)'>2</a>
<a class="btn btn-primary" id="3" ng-click='updateOutput(3)' >3</a>
<a class="btn btn btn btn-warning" id="-" ng-click='subtract()'>-</a>
<br>
<a class="btn btn-primary bigButton" id="0" ng-click='updateOutput(0)'>0</a>
<a class="btn btn-primary" id="dot" ng-click='updateOutput(".")'>.</a>
<a class="btn btn-primary btn-warning" id="evaluate" ng-click='evaluate()'>=</a>
</html>